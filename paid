        webpay.noRegisteredCardsMsg = 'You have no Verve™ cards registered. Register your Verve™ card on the ATM so that you wont have to remember your card number.';

       
        
        //{
        var value = Model != null && Model.QuickTellerUserPassportDetails != null ? "0;" : "-1;";
        //}
        webpay.emptyCardSelectValue = value;  

        var paymentStarted = false;

        $('#returntomerchantsite').click(function() {
            return ReturnToMerchantSite();
        });


        function ReturnToMerchantSite() {
            if (paymentStarted || confirm('Are you sure you want to exit? Your purchase has not completed. Click Cancel to return to WebPAY. Click OK to return to the merchant site.')) {
                return true;
            }
            return false;
        }
        
        function Cancel_Click() {
            $('#returntomerchantsite').click();
        }

    </script>
</body>
</html>
